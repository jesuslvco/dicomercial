<?xml version='1.0' encoding='utf-8'?>
<widget id="org.mdm_mobile" version="1.0.0" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <name>dicomercial</name>
    <description>
        Directorio comercial del centro del pa√≠s.
    </description>
    <author email="dev@cordova.apache.org" href="http://cordova.io">
        Calvillo Publicidad
    </author>
    <content src="index.html" />
    <access origin="*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
    <platform name="android">
        <allow-intent href="market:*" />
    </platform>
    <platform name="ios">
        <allow-intent href="itms:*" />
        <allow-intent href="itms-apps:*" />
    </platform>
    <engine name="android" spec="^7.0.0" />
    <plugin name="cordova-plugin-whitelist" spec="^1.3.3" />
    <plugin name="com.teamnemitoff.phonedialer" spec="~0.3.1" />
    <plugin name="cordova-plugin-x-socialsharing" spec="^5.4.1" />
</widget>
